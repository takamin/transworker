{
  "name": "transworker",
  "version": "1.0.7",
  "description": "An inter thread method invoker. This module yields the interfaces for the main thread to invoke the instance methods of the class by peeping that prototype.",
  "main": "index.js",
  "scripts": {
    "build": "parcel build --out-dir . --out-file transworker.js index.js",
    "sample": "node sample/webserver.js",
    "test": "node bin/build-test.js && parcel test/test.html --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takamin/transworker.git"
  },
  "keywords": [
    "WebWorker",
    "Multi-Thread"
  ],
  "author": "Koji Takami",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/takamin/transworker/issues"
  },
  "homepage": "http://takamints.hatenablog.jp/entry/2016/01/31/mutithreading-javascript-on-a-browser-using-a-web-workers-api",
  "devDependencies": {
    "chai": "^4.2.0",
    "connect": "^3.6.0",
    "eslint": "^4.19.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "opn": "^4.0.2",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-eslint": "^1.0.4",
    "parcel-plugin-jsdoc": "0.0.1",
    "serve-static": "^1.12.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "uuid": "^3.3.2"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-console": "off",
      "valid-jsdoc": "warn"
    },
    "extends": "eslint:recommended"
  }
}
